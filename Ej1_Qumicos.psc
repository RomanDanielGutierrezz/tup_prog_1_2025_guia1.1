Algoritmo qumicos
	Definir a, b ,mla ,mlb como Real;
	Definir ml, L Como Real;
	Escribir 'Indique la cantidad de mililitros requeridos :';
	Leer ml;
	mla=(2.55/1000);
	mlb=(5.78/1000);
	a=(mla*ml);
	b=(mlb*ml);
	Escribir 'La cantidad de gramos que debe agregar son :';
	Escribir 'Quimico A ',a 'g';
	Escribir 'Quimico B ',b 'g';
FinAlgoritmo
